{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "dist",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },

  "include": ["src/**/*", "src/**/*.json", "src/*.proto"],
  "exclude": ["node_modules"]
}